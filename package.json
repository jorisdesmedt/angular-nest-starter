{
  "name": "angular-nest",
  "version": "0.0.0",
  "scripts": {
    "start": "node dist/server/server.js",
    "start:dev:client": "run-p server:start client:start",
    "start:dev:server": "npm run build:client && npm run build:client:ssr && nodemon",
    "build": "npm run build:client && npm run build:client:ssr && npm run build:server",
    "build:client": "ng build",
    "build:client:ssr": "ng run client:server",
    "build:server": "webpack --config webpack.config.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "jest --config jest.client.config.js --coverage",
    "test:server": "jest --config jest.server.config.json --coverage",
    "e2e": "npm run e2e:client && npm run e2e:server",
    "e2e:client": "ng e2e",
    "e2e:server": "jest --config ./e2e/server/jest-e2e.json",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "ng lint",
    "lint:server": "tslint -c server/tslint.json 'server/src/**/*.ts'",
    "prettier": "prettier --write \"**/*.ts\"",
    "prettier:check": "prettier --loglevel error \"**/*.ts\"",
    "server:start": "ts-node --project server/tsconfig.json -r tsconfig-paths/register server/src/main.ts",
    "client:start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^6.0.9",
    "@angular/router": "^6.0.3",
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/passport": "^1.1.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@types/passport-jwt": "^3.0.1",
    "bcrypt": "^2.0.1",
    "config": "^1.31.0",
    "core-js": "^2.5.4",
    "fastify-formbody": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "nest-router": "^1.0.6",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@nestjs/testing": "^5.0.0",
    "@types/bcrypt": "^2.0.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.0.39",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^21.1.8",
    "@types/node": "~8.9.4",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.6",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^21.2.1",
    "jest-preset-angular": "5.2.3",
    "nodemon": "^1.14.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.11.1",
    "protractor": "~5.3.0",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.4.2",
    "ts-node": "~5.0.1",
    "tsconfig-paths": "^3.1.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
}
